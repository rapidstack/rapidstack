{
  "name": "@rapidstack/lambda",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit -p tsconfig.build.json && tsup && tsc",
    "test": "vitest",
    "watch": "tsc --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/aws-lambda": "8.10.137",
    "@types/node": "20.12.5",
    "vitest": "1.4.0"
  },
  "files": [
    "dist/*",
    "!**/*.spec.*",
    "!**/*.test.*",
    "!**/tests/*.*",
    "!**/tests/*.ts.map"
  ],
  "dependencies": {
    "pino": "~8.20.0",
    "valibot": "~0.30.0"
  },
  "peerDependencies": {
    "valibot": "^0.29.0"
  }
}
