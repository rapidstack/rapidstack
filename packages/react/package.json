{
  "name": "@rapidstack/react",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit -p tsconfig.build.json && tsup && tsc",
    "test": "vitest",
    "watch": "tsc --watch"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "devDependencies": {
    "@ladle/react": "^4.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
