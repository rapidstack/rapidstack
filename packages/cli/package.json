{
  "name": "@rapidstack/cli",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:init": "tsc --noEmit -p tsconfig.build.json && tsup --silent",
    "pre-build": "tsup",
    "build": "tsc --noEmit && tsup",
    "test": "vitest",
    "watch": "tsc --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./create": {
      "import": "./dist/commands/create/index.js",
      "types": "./dist/commands/create/index.d.ts"
    },
    "./create-plugin": {
      "import": "./dist/commands/create-plugin/index.js",
      "types": "./dist/commands/create-plugin/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "rapidstack": "./bin/rapidstack-cli.mjs"
  },
  "devDependencies": {
    "@rapidstack/test-utils": "workspace:*",
    "@types/glob": "8.1.0",
    "@types/inquirer": "9.0.7",
    "@types/node": "20.12.12",
    "vitest": "1.5.3"
  },
  "files": [
    "dist/*",
    "bin/*",
    "templates/*",
    "!dist/local.*",
    "!**/*.spec.*",
    "!**/*.test.*",
    "!**/tests/*.*",
    "!**/tests/*.ts.map"
  ],
  "dependencies": {
    "chalk": "~5.3.0",
    "commander": "~12.0.0",
    "glob": "~10.3.12",
    "inquirer": "~9.2.20",
    "node-fetch": "~3.3.2",
    "ora": "~8.0.1"
  }
}
